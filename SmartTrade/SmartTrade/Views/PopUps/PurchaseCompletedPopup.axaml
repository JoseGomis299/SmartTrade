<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="393" d:DesignHeight="500"
             x:Class="SmartTrade.Views.PurchaseCompletedPopup"
             xmlns:vm="clr-namespace:SmartTrade.ViewModels"
             xmlns:controls="clr-namespace:SmartTrade.Controls">
  <StackPanel Margin="10 10" HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
    <TextBlock Classes="h2" VerticalAlignment="Center" HorizontalAlignment="Center" Text="Purchase completed correctly!"/>
    <Button HorizontalAlignment="Right" Height="40" Width="100" Margin="0 20 0 0" Classes="Green" Name="AcceptButton">OK</Button>
  </StackPanel>
</UserControl>
